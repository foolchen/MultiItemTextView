<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="MultiItemTextView">
    <attr format="integer" name="mitv_count"/>
    <attr format="color" name="mitv_color"/>
    <attr format="dimension" name="mitv_text_size"/>
    <attr format="dimension" name="mitv_item_width"/>
    <attr format="dimension" name="mitv_item_height"/>
    <attr format="dimension" name="mitv_item_padding"/>
    <attr format="dimension" name="mitv_item_padding_start"/>
    <attr format="dimension" name="mitv_item_padding_top"/>
    <attr format="dimension" name="mitv_item_padding_end"/>
    <attr format="dimension" name="mitv_item_padding_bottom"/>
    <attr format="dimension" name="mitv_divider_width"/>
    <attr format="boolean" name="mitv_start_divider_enable"/>
    <attr format="boolean" name="mitv_top_divider_enable"/>
    <attr format="boolean" name="mitv_end_divider_enable"/>
    <attr format="boolean" name="mitv_bottom_divider_enable"/>
    <attr format="boolean" name="mitv_middle_divider_enable"/>
    <attr format="boolean" name="mitv_divider_equally"/>
    <attr format="boolean" name="mitv_width_contain_divider"/>
    <attr format="color" name="mitv_divider_color"/>
    <attr format="string" name="mitv_texts"/>
    <attr format="enum" name="mitv_gravity">
      <enum name="none" value="0"/>
      <enum name="center_vertical" value="1"/>
      <enum name="center" value="2"/>
    </attr>
    <attr format="dimension" name="mitv_ref_item_width"/>
  </declare-styleable>
</resources>